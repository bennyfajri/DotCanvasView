<resources>
    <style name="BottomNavigationViewTextStyle_active" parent="Widget.Design.BottomNavigationView">
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="BottomNavigationViewTextStyle_inactive" parent="BottomNavigationViewTextStyle_active">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="input_layout_style" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="hintEnabled">false</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
    </style>

    <style name="input_layout_spinner" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="hintEnabled">false</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
    </style>

    <style name="input_edit_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="button_text_style12" parent="text_semi_bold12">
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="cornerRadius">8dp</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="button_text_style14" parent="button_text_style12">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="button_text_style16" parent="button_text_style12">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="button_text_style18" parent="button_text_style12">
        <item name="android:textSize">18sp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingStart">24dp</item>
        <item name="android:paddingEnd">24dp</item>
    </style>

    <style name="profile_button_style" parent="Widget.Material3.Button.TextButton">
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:fontFamily">@font/quicksand_medium</item>
        <item name="cornerRadius">0dp</item>
        <item name="android:gravity">start</item>
        <item name="android:padding">16dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:textColor">@android:color/darker_gray</item>
        <item name="iconSize">24dp</item>
        <item name="iconPadding">12dp</item>
        <item name="iconTint">@null</item>
    </style>

    <style name="text_size">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <!--    Text Bold       -->
    <style name="text_bold10" parent="text_size">
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="text_bold12" parent="text_bold10">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text_bold14" parent="text_bold10">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_bold16" parent="text_bold10">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="text_bold18" parent="text_bold10">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="text_bold20" parent="text_bold10">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="text_bold22" parent="text_bold10">
        <item name="android:textSize">22sp</item>
    </style>

    <style name="text_bold24" parent="text_bold10">
        <item name="android:textSize">24sp</item>
    </style>

    <style name="text_bold28" parent="text_bold10">
        <item name="android:textSize">28sp</item>
    </style>


    <!--    Text Semi Bold   -->
    <style name="text_semi_bold10" parent="text_size">
        <item name="android:fontFamily">@font/quicksand_medium</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="text_semi_bold12" parent="text_semi_bold10">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text_semi_bold13" parent="text_semi_bold12">
        <item name="android:textSize">13sp</item>
    </style>

    <style name="text_semi_bold14" parent="text_semi_bold12">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_semi_bold16" parent="text_semi_bold14">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="text_semi_bold18" parent="text_semi_bold14">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="text_semi_bold20" parent="text_semi_bold14">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="text_semi_bold22" parent="text_semi_bold14">
        <item name="android:textSize">22sp</item>
    </style>

    <style name="text_semi_bold24" parent="text_semi_bold14">
        <item name="android:textSize">24sp</item>
    </style>

    <style name="text_semi_bold28" parent="text_semi_bold14">
        <item name="android:textSize">28sp</item>
    </style>


    <!--    Text Regular        -->
    <style name="text_regular10" parent="text_size">
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="text_regular11" parent="text_regular10">
        <item name="android:textSize">11sp</item>
    </style>

    <style name="text_regular12" parent="text_regular10">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text_regular14" parent="text_regular10">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_regular16" parent="text_regular10">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="text_regular18" parent="text_regular10">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="text_regular20" parent="text_regular10">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="text_regular22" parent="text_regular10">
        <item name="android:textSize">22sp</item>
    </style>

    <style name="text_regular24" parent="text_regular10">
        <item name="android:textSize">24sp</item>
    </style>

    <style name="text_regular30" parent="text_regular10">
        <item name="android:textSize">30sp</item>
    </style>

    <style name="outlinedButton_Bold" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:insetTop">2dp</item>
        <item name="android:insetBottom">2dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="iconSize">20dp</item>
        <item name="strokeColor">@color/overlay_light_primary</item>
        <item name="iconGravity">textStart</item>
        <item name="cornerRadius">8dp</item>
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="changeButtonStyle" parent="button_text_style14">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/overlay_light_primary</item>
        <item name="iconSize">20dp</item>
        <item name="iconTint">@color/overlay_light_primary</item>
        <item name="cornerRadius">8dp</item>
    </style>
</resources>